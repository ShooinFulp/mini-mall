<!--pages/home/childCpns/f-recommend/f-recommend.wxml-->
<view class="recommend">
  <block wx:for="{{recommends}}" wx:key="index">
    <view>
      <view>
        <image src="{{item.image}}" mode="widthFix" bindload="imgLoad"></image>
      </view>
      <view>
        <text>{{item.title}}</text>
      </view>
    </view>
  </block>
</view>
